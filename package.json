{
  "name": "pegasus",
  "version": "0.0.2",
  "description": "Heavyweight, newbie-friendly Minecraft launcher.",
  "author": "GrieferPig <grieferpig@163.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn add electron && vue-cli-service build",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "release": "standard-version && npm run pushtags",
    "alpharel": "standard-version --prerelease alpha && npm run pushtags",
    "beta": "standard-version --prerelease beta && npm run pushtags",
    "pushtags": "git push --follow-tags github Indev"
  },
  "main": "background.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@mdi/font": "5.9.55",
    "core-js": "^3.8.3",
    "follow-redirects": "^1.14.9",
    "node-gyp": "^9.0.0",
    "request-progress": "^3.0.0",
    "roboto-fontface": "*",
    "ts-loader": "~8.2.0",
    "ts-node": "^10.7.0",
    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vuetify": "^3.0.0-beta.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/webfontloader": "^1.0.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "electron": "^17.2.0",
    "electron-devtools-installer": "^3.1.0",
    "sass": "^1.38.0",
    "sass-loader": "^10.0.0",
    "standard-version": "^9.3.2",
    "typescript": "~4.5.5",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-vuetify": "~2.4.8",
    "vuetify-loader": "^2.0.0-alpha.0"
  }
}
